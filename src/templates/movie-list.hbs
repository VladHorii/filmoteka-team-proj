<li class='card'>
  <img
    class='card_img'
    src='https://image.tmdb.org/t/p/w500{{this.posterPath}}'
    alt='{{this.title}}'
    width='320'
  />
  <h2>
    {{this.title}}
  </h2>
  <p>
    {{genre_ids}}|
  </p>
  <div>
    {{this.relDate}}
  </div>
</li>
